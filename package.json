{
  "name": "haricot",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:clear": "expo start --clear",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "android:dev": "expo run:android --device",
    "android:emulator": "expo run:android --no-build-cache",
    "android:clean": "rm -rf android/app/.cxx && cd android && ./gradlew clean && cd ..",
    "convex:dev": "npx convex dev",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:simulator": "expo run:ios --simulator",
    "ios:clean": "cd ios && xcodebuild clean && cd ..",
    "web": "expo start --web",
    "web:build": "expo export:web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:ios:local": "eas build --platform ios --local",
    "build:android:local": "eas build --platform android --local",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "npx expo install --fix",
    "update:check": "npx expo install --check",
    "install:fix": "npm install && npx expo install --fix",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "clean": "rm -rf node_modules && rm -rf .expo && rm -rf dist && rm -rf ios/build && rm -rf android/build && rm -rf android/app/build",
    "clean:cache": "rm -rf .expo && rm -rf node_modules/.cache",
    "clean:ios": "rm -rf ios/build && rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean:android": "rm -rf android/build && rm -rf android/app/build && rm -rf android/app/.cxx && rm -rf android/.gradle",
    "reset": "npm run clean && npm install && npx expo install --fix",
    "reset-project": "node ./scripts/reset-project.js",
    "patch:expo-cli": "node scripts/patch-expo-cli.js",
    "postinstall": "node scripts/patch-expo-cli.js",
    "maildev": "maildev --smtp 1025 --web 1080 --disable-webhook"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.90",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "convex": "^1.28.0",
    "d3-geo": "^3.1.1",
    "expo": "~54.0.27",
    "expo-application": "~7.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.20",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.9",
    "expo-linking": "~8.0.10",
    "expo-localization": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.17",
    "expo-secure-store": "~15.0.8",
    "expo-speech-recognition": "^2.1.5",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.6.0",
    "nodemailer": "^6.9.11",
    "posthog-react-native": "^4.14.3",
    "posthog-react-native-session-replay": "^1.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.2.2",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "replicate": "^1.3.1",
    "resend": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "maildev": "^2.1.0",
    "react-native-svg-transformer": "^1.5.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
